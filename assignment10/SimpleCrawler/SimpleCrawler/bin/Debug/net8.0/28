    <script>
      const isAliyunDomain = /^([^.]+\.)*aliyun\.com$/.test(location.hostname);

      if(isAliyunDomain){
         const aes = new AES({
          pid: 'f8hD7i',
          user_type: '6',
          uid: (function getUID() {
            var cpk = document.cookie.match(/login_current_pk=(.*?);/);
            var pk = document.cookie.match(/login_aliyunid_pk=(.*?);/);
            return cpk && cpk.length === 2
              ? cpk[1]
              : pk && pk.length === 2
              ? pk[1]
              : '';
          })(),
          env: 'prod',
          dim1: 'old-homon',
        });
        window.__AES_PLUGINS__ = {
          pv: aes.use(AESPluginPV),
          event: aes.use(AESPluginEvent),
          jsError: aes.use(AESPluginJSError),
          api: aes.use(AESPluginAPI),
